{
  "store.custom#landing": {
    "blocks": ["image", "search-result-layout.customQuery"]
  },
  "image": {
    "props": {
      "minWidth": "100%"
    }
  },
  "search-result-layout.customQuery": {
    "blocks": ["search-result-layout.desktop"],
    "props": {
      "querySchema": {
        "orderByField": "OrderByReleaseDateDESC",
        "hideUnavailableItems": true,
        "maxItemsPerPage": 8
      }
    }
  }
}
